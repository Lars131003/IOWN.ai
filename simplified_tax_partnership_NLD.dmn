    </informationRequirement>
    <literalExpression id="LiteralExpression_0848h9j">
      <text>flatten(for c in rChildrenParents return if c.id = rCohabitation
then 
  {
    "id":c.id,
    "OwnershipPercentage":c.OwnershipPercentage
  }
   else [])</text>
    </literalExpression>
  </decision>
